{
  "expo": {
    "name": "Torob Mobile",
    "slug": "torob-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "icon": "./assets/icon.png",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.torob.mobile"
    },
    "android": {
      "package": "com.torob.mobile"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "scheme": "torob",
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false
    }
  }
}
